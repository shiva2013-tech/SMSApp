@model List<Qualification>

@if (Model != null && Model.Any())
{
    <table class="table">
        <thead>
            <tr>
                <th>Course</th>
                <th>University</th>
                <th>Year</th>
                <th>Percentage</th>
                <!-- Add more columns as needed -->
            </tr>
        </thead>
        <tbody>
            @foreach (var qualification in Model)
            {
                <tr>
                    <td>@qualification.Course</td>
                    <td>@qualification.University</td>
                    <td>@qualification.Year</td>
                    <td>@qualification.Percentage%</td>
                    <!-- Add more columns as needed -->
                </tr>
            }
        </tbody>
    </table>
}
else
{
    <p>No qualifications available</p>
}
